      <Спецификация Ref="6" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Спецификация</Alias>
        <Columns isList="true" count="11">
          <value>idorderitem,System.Int32</value>
          <value>idproductiontype,System.Int32</value>
          <value>item_name,System.String</value>
          <value>add_name1,System.String</value>
          <value>add_name2,System.String</value>
          <value>item_color,System.String</value>
          <value>item_qu,System.Decimal</value>
          <value>item_size,System.String</value>
          <value>pricebase,System.Decimal</value>
          <value>smbase,System.Decimal</value>
          <value>smbase3,System.Decimal</value>
        </Columns>
        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Спецификация</Name>
        <NameInSource>DefaultConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>select	isnull(oi.idorderitem,0) idorderitem,
    isnull(oi.idproductiontype,0) idproductiontype,
    ( case
           when oi.idproductiontype = 660 then isnull(ct.name,'')
           when oi.idproductiontype = 680 then isnull(oi.name,'') + '. ' + isnull(ct.name,'')
           when oi.idproductiontype = 820 then oi.comment + ' '
when oi.idproductiontype = 760 then oi.name + ' ' +cast(isnull(( select max(cast(mpc.intvalue as int)) from modelparamcalc mpc where mpc.idorderitem = oi.idorderitem and mpc.idmodelparam = 1020 and mpc.deleted is null ),0) as varchar)+ 'мм. ' 		
           when pt.typ = 1 then isnull(oi.name,'') + '. ' + isnull(ct.name,'') + ' ' + isnull(ps.name,'')
           else isnull(pt.name,'')
      end ) item_name,
    ( case
          when oi.idproductiontype = 460 then ' (' + isnull(fs.name,'') + ')'
when oi.idproductiontype = 500 then ' ' + isnull(( select max(mpc.strvalue) from modelparamcalc mpc where mpc.idorderitem = oi.idorderitem and mpc.idmodelparam = 360 and mpc.deleted is null ),'')
when oi.idproductiontype = 740 then ' (' + isnull(( select max(mpc.strvalue) from modelparamcalc mpc where mpc.idorderitem = oi.idorderitem and mpc.idmodelparam = 1000 and mpc.deleted is null ),'') + ')'
		  else pt.comment
      end ) add_name1,
    ( case
          when oi.idproductiontype = 500 then '' 
          else ''
      end ) add_name2,
    ( case
           when oi.idproductiontype = 820 then ' ' + isnull(ISNULL(cout.name, ''),'') + ' / ' + isnull(ISNULL(cin.name, ''),'')
           when oi.idproductiontype in (680,780,800,840,860,243,217) then ''
           when oi.idproductiontype in (select idproductiontype from dbo.productiontype where idproductiontypegroup=-2147483640) then ''
           when oi.idcolorout is null and oi.idcolorin is null then ''
           when oi.idcolorout is not null and oi.idcolorin is null then ', ' + isnull(ISNULL(cout.name, ''),'')
           when oi.idcolorout is null and oi.idcolorin is not null then ', ' + isnull(ISNULL(cin.name, ''),'')
           else ', ' + isnull(ISNULL(cout.name, ''),'') + ' / ' + isnull(ISNULL(cin.name, ''),'')
      end ) item_color,
   isnull(oi.qu,0) item_qu,
-- &lt;mart 30.08.2011
( case when oi.idproductiontype = 700 then cast(oi.width as varchar) + ' x ' + cast(oi.height as varchar)--(select cast(max(mc.width) as varchar) + ' x ' + cast(max(mc.height) as varchar) from modelcalc mc
											--	   where mc.idorderitem = oi.idorderitem and mc.deleted is null )
when oi.idproductiontype = 760 then cast(oi.thick as varchar)--(select cast(max(mc.thick) as varchar) from modelcalc mc where mc.idorderitem = oi.idorderitem and mc.deleted is null )
-- mart&gt; 30.08.2011
when oi.idproductiontype = 500 then cast(oi.thick as varchar) + ' x ' + cast(isnull(( select max(cast(mpc.intvalue as int)) from modelparamcalc mpc where mpc.idorderitem = oi.idorderitem and mpc.idmodelparam = 380 and mpc.deleted is null ),0) as varchar)
          when oi.thick is not null then cast(oi.thick as varchar)
          when oi.height is not null then cast(oi.height as varchar) + ' x ' + cast(oi.width as varchar)
          else ''
     end ) item_size,
   isnull(oi.pricebase,0) pricebase,
   isnull(oi.smbase,0) smbase,
   isnull(oi.smbase3,0) smbase3
from view_orderitem oi
LEFT OUTER JOIN dbo.constructiontype AS ct ON ct.idconstructiontype = oi.idconstructiontype 
LEFT OUTER JOIN dbo.productiontype AS pt ON oi.idproductiontype = pt.idproductiontype 
LEFT OUTER JOIN  dbo.system AS fs ON oi.idfurnsys = fs.idsystem 
LEFT OUTER JOIN dbo.color AS cin ON oi.idcolorin = cin.idcolor
LEFT OUTER JOIN dbo.color AS cout ON oi.idcolorout = cout.idcolor
LEFT OUTER JOIN dbo.system AS ps ON oi.idprofsys = ps.idsystem 
where 1=1
and oi.deleted is null
and oi.idproductiontype in (select idproductiontype from dbo.productiontype where idproductiontypegroup&gt;0)
and oi.idproductiontype NOT in (select idproductiontype from dbo.productiontype where idproductiontypegroup in (80,200,220,240,260,280,380,864,865,866,867,868,869,100000,100001,100002,100003,100004))
and oi.idorder = {idorder}
and (oi.productiontype_productiontypegroup!='нест' or oi.productiontype_productiontypegroup is null)

and oi.ismoskit=0

union all
select
	isnull(mc.idorderitem,0) idorderitem,
    0 idproductiontype,
    isnull(mc.good_name,'') + ' (' +isnull(mc.good_marking,'') + ')' item_name,
    '' add_name1,
    '' add_name2,
    ( case
           when mc.idcolor1 is null and mc.idcolor2 is null then ''
           when mc.idcolor1 is not null and mc.idcolor2 is null then ', ' + isnull(c1.name,'')
           when mc.idcolor1 is null and mc.idcolor2 is not null then ', ' + isnull(c2.name,'')
           when mc.idcolor1 = mc.idcolor2 then ', ' + isnull(c1.name,'')
           else ', ' + isnull(c1.name,'') + ' / ' + isnull(c2.name,'')
      end ) item_color,
        isnull(case mc.idgood when 2783then mc.qu else mc.orderitem_qu end ,0) item_qu,
   ( case
          when mc.thick is not null then cast(mc.thick as varchar)
          when mc.height is not null then cast(mc.height as varchar) + ' x ' + cast(mc.width as varchar)
          else ''
     end ) item_size,
    0 pricebase,
    0 smbase,
    0 smbase3
from view_modelcalc mc 
     join orderitem oi on mc.idorderitem = oi.idorderitem and oi.deleted is null
     left join color c1 on mc.idcolor1 = c1.idcolor
     left join color c2 on mc.idcolor2 = c2.idcolor
where 1=1
and mc.deleted is null 
and mc.good_idgoodtype = 4 
and oi.idproductiontype in (460, 560, 580, 600, 620, 640, 1720)
and mc.good_name not like '%Подстав%'and mc.good_name not like'лента ПТЮ%'
and mc.idorder = {idorder}</SqlCommand>
